// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model myprofile {
  id        Int      @id @default(autoincrement())
  img       String
  username  String
}

model friends {
  id        Int      @id @default(autoincrement())
  img       String
  username  String
  birth     String
  phone     String
}